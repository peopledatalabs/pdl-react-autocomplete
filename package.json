{
  "name": "pdl-react-autocomplete",
  "version": "2.1.1",
  "description": "A react component for the People Data Labs Autocomplete API",
  "packageManager": "pnpm@10.12.1",
  "source": "src/index.ts",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "includeNodeModules": false,
      "isLibrary": true
    },
    "module": {
      "includeNodeModules": false,
      "isLibrary": true
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "eslint src --fix",
    "dev": "parcel watch -p 3006",
    "build": "parcel build",
    "pub": "pnpm run build && pnpm publish --no-git-checks --provenance --access public",
    "check-packages": "pnpm dlx npm-check-updates"
  },
  "repository": "https://github.com/peopledatalabs/pdl-react-autocomplete.git",
  "keywords": [
    "peopledatalabs",
    "People Data Labs",
    "autocomplete",
    "react",
    "react component"
  ],
  "author": "People Data Labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peopledatalabs/pdl-react-autocomplete/issues"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.15.4",
    "@parcel/transformer-react-refresh-wrap": "^2.15.4",
    "@parcel/transformer-typescript-types": "^2.15.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "npm-check-updates": "^18.0.1",
    "parcel": "^2.15.4",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract"
    ]
  }
}
